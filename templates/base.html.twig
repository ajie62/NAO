<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Nos Amis Les Oiseaux{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#menu">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="logoContainer navbar-brand ">
                        <a href="{{ path('app.homepage') }}" class=""><img src="{{ asset('images/nao.png') }}"></a>
                        <div class="navbar-text">
                            <p class="logo "><strong>Nos amis les oiseaux</strong><br>En route vers un monde meilleur</p>
                        </div>
                    </div>
                    {#<ul class="nav navbar-nav">#}
                        {#<li> </li>#}
                        {#<li ></li>#}
                    {#</ul>#}
                </div>
                <div class="collapse navbar-collapse" id="menu">
                    <ul class="nav navbar-nav navbar-text navbar-right">
                        <li><a class="current" href="{{ path('app.homepage') }}">Accueil</a></li>
                        <li><a href="{{ path('observation.search') }}">Observations</a></li>
                        <li><a href="{{ path('blog.list_article') }}">Blog</a></li>
                        <li><a href="{{ path('app.homepage') }}">Contact</a></li>
                        <li><a href="{{ path('app.homepage') }}">Connexion</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('vendor/jquery/jquery-3.2.1.min.js') }}"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</html>
